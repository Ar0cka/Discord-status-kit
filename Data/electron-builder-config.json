{
  "appId": "ara.fakerpc",
  "productName": "FakeRtcForDis",
  "icon": "src/img/discord-anime-app-icon.ico",
  "files": [
    "src/**/*",
    "python-embed/**/*",
    "python/**/*"
  ],
  "extraResources": [],
  "asar": false,
  "npmRebuild": false,
  "win": {
    "icon": "src/img/discord-anime-app-icon.ico",
    "target": [
      {
        "target": "portable",
        "arch": [
          "x64"
        ]
      },
      {
        "target": "nsis",
        "arch": [
          "x64"
        ]
      }
    ],
    "artifactName": "${productName}-${version}-${platform}-Setup.${ext}"
  },
  "portable": {
    "artifactName": "${productName}-Portable-${version}.${ext}"
  },
  "nsis": {
    "oneClick": false,
    "allowToChangeInstallationDirectory": true
  }
}
